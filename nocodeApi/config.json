  "projectProgram":{
    "/":{
      "version":"2에서 재고예정",
      "method":"get",
      "allow":{"user-agent":"mozilla","accept":"aplication/json"}
      ,"deny":{"user-agent":"mozilla","accept":"aplication/json"}
      ,"process":[
        {"var1":{
          "type":"sql"
          ,"sql":"SELECT * FROM borad WHERE seq=%s"
          }
        }
        ,{"var3":{
          "type":"method"
          ,"class":"abc"
          ,"method":"abc"
          ,"param":"var1"
          }
        }
        ,{"var4":{
          "type":"curl"
          ,"url":"http://test.com?abc=1&bbb=2"
          ,"method":"post"
          ,"post":{"form1":"value1"}
          ,"body":{"json2":"value2"}
          }
        }
        ]
      ,"return":"SELECT * FROM borad WHERE seq={:params.seq}"
    }
  },
    "endpoints": [
      {
        "path": "/",
        "method": "GET",
        "handler": "read_root",
        "params": [],
        "func":{"message": "Hello World"}
      },
      {
        "path": "/items/{item_id}",
        "method": "GET",
        "handler": "read_item",
        "params": ["item_id","item_id2"],
        "func":{"message": "Hello World"}
      }
    ]
  }